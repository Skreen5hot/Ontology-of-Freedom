@prefix ex: <http://example.org/moral_sandbox#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Agents
ex:Gemini a ex:Agent ;
    rdfs:label "Gemini AI Agent" .

ex:Alice a ex:Agent ;
    rdfs:label "Alice" .

# Artifacts
ex:Wallet1 a ex:Artifact ;
    ex:ownedBy ex:Alice ;
    rdfs:label "Alice's Wallet" .

# Actions
ex:PickUpWallet a ex:Action ;
    ex:performedBy ex:Gemini ;
    ex:actsOn ex:Wallet1 ;
    ex:realizesIntent ex:Intent_Return .

ex:ReturnWalletToOwner a ex:Action ;
    ex:performedBy ex:Gemini ;
    ex:actsOn ex:Wallet1 ;
    ex:realizesIntent ex:Intent_Return ;
    ex:hasMoralValue ex:Fairness, ex:Trust .

# Intents
ex:Intent_Return a ex:Intent ;
    rdfs:label "Return to rightful owner" .

ex:Intent_Keep a ex:Intent ;
    rdfs:label "Keep for oneself" .

# Moral Values
ex:Fairness a ex:Value ; 
    rdfs:label "Fairness" .
ex:Trust a ex:Value ; 
    rdfs:label "Trust" .
ex:Compassion a ex:Value ;
    rdfs:label "Compassion" .

# Moral Evaluation for the "return" action
ex:Eval_ReturnWallet a ex:MoralEvaluation ;
    rdfs:label "Evaluation for returning the wallet" ;
    ex:evaluatesAction ex:ReturnWalletToOwner ;
    ex:assignsValue ex:Fairness, ex:Trust, ex:Compassion ;
    ex:justificationText "Returning a lost item to its owner is an act that promotes fairness, builds trust, and shows compassion." .